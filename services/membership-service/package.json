  "description": "An open source community engagement platform",
  "main": "index.js",
  "author": "Adam Colpitts",
  "license": "MIT",
  "scripts": {
    "dev": "sls offline start --httpPort 4002 --lambdaPort 4042",
    "check:updates": "npx npm-check-updates",
    "check:updates:u": "npx npm-check-updates -u && pnpm i",
    "deploy:dev": "serverless deploy --verbose",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:production": "serverless deploy --aws-profile $AWS_PROFILE --stage production",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint '**/*.ts' --fix",
    "package": "serverless package",
    "db:seed": "ts-node ./scripts/seed-data.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@serverless/eslint-config": "^5.1.0",
    "@types/http-errors": "^2.0.2",
    "@types/jest": "^29.5.5",
    "@types/supertest": "^2.0.14",
    "@types/uuid": "^9.0.5",
    "esbuild": "^0.19.4",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.0.1",
    "serverless-domain-manager": "^7.1.2",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.48.4",
    "serverless-offline": "^13.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsconfig": "*",
    "typescript": "^5.2.2"
  },
  "dependencies": {
